{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "src/**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "types": ["react", "react-dom"],
    "paths": {
      "baseUrl": ["."],
      "@layout/*": [
        "./src/scss/layout/*"
      ],
      "@core/*": [
        "./src/scss/core/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@style_components/*": [
        "./src/scss/components/*"
      ],
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}